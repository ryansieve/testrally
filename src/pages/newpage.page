<apex:page id="newpage" standardController="Account" extensions="NewPageCon">
    <apex:pageBlock>
        <apex:pageBlockSection>
            <apex:pageBlockSectionItem>
                <apex:outputLabel value="Current Time"/>
                <apex:outputText value="{!currentTime}"/>
            </apex:pageBlockSectionItem>
            <apex:pageBlockSectionItem>
                <apex:outputLabel value="User ID"/>
                <apex:outputText value="{!userId}"/>
            </apex:pageBlockSectionItem>
        </apex:pageBlockSection>
    </apex:pageBlock>

</apex:page>
